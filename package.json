{
  "name": "helios",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "deploy": "npm run migrate:remote && npx wrangler deploy",
    "start": "npx wrangler dev",
    "migrate:local": "npx wrangler d1 migrations apply dev_pmo --local",
    "migrate:remote": "npx wrangler d1 migrations apply dev_pmo --remote",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "test": "jest"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230419.0",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.38.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "wrangler": "^4.43.0"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.7.4",
    "hono": "^3.11.0",
    "zod": "^4.1.12"
  }
}
